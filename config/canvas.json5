{
        //
	// Controls the bloom system for canvas.
	//
	"enableBloom": false,
	"bloomIntensity": 0.20,
	"bloomScale": 0.25,
	//
	// Animates the specific blocks.
	//
	"wavyGrass": false,
	//
	// Controls the light radius when holding lit-up items/blocks.
	//
	"handheldLightRadius": 5,
	//
	// Controls the fog system for canvas.
	//
	"fogMode": "VANILLA",
